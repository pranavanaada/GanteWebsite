# EmailJS configuration (get these from https://dashboard.emailjs.com/)
VITE_EMAILJS_SERVICE_ID=
VITE_EMAILJS_TEMPLATE_ID=
VITE_EMAILJS_PUBLIC_KEY=

# Serverless email via Resend (recommended)
RESEND_API_KEY=
RESEND_FROM=onboarding@resend.dev
CONTACT_TO_EMAIL=pranavanaadaa@gmail.com
ALLOWED_ORIGIN=*

# Frontend dev proxy (optional)
# Point Vite dev server to your API while developing locally
VITE_PROXY_TARGET=

# Direct frontend to a specific API base (optional)
# If set, the app will POST to {VITE_API_BASE}/api/contact instead of /api/contact
VITE_API_BASE=
